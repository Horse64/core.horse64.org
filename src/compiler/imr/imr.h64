        _passthrough_storage_refs=no,
        ) {
    var obj = base.as_json_obj(
        _passthrough_storage_refs=
            _passthrough_storage_refs)
    obj["value"] = self.value
    if _passthrough_storage_refs {
        obj["target_storage_ref"] =
            self.target_storage_ref
    } else {
